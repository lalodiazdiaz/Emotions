{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "overrides": [],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-param-reassign": [2, { "props": false }],
    "no-unused-vars": "off",
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "always"
      }
    ],
    "lit-a11y/click-events-have-key-events": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": "off",
    "react/jsx-curly-spacing": [
      2,
      {
        "allowMultiline": false,
        "children": true
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-no-bind": [
      "off",
      {
        "ignoreRefs": true
      }
    ],
    "react/jsx-sort-props": "error",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-props-no-spreading": "off",
    "react/jsx-indent": [
      "error",
      "tab"
    ],
    "react/jsx-indent-props": [
      "error",
      "tab"
    ],
    "indent": [
      "error",
      "tab"
    ],
    "max-len": [
      "error",
      {
        "code": 90,
        "tabWidth": 2
      }
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ],
    "operator-linebreak": [
      "error",
      "before",
      {
        "overrides": {
          "=": "after"
        }
      }
    ],
    "default-param-last": 0,
    "sort-keys": "error",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [],
      "specialLink": [ "hrefLeft", "hrefRight" ],
      "aspects": [ "noHref", "invalidHref", "preferButton" ]
    }],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-mixed-spaces-and-tabs": 0,
    "no-tabs": "off"
  }
}